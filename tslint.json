{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": ["node_modules/**/*", "build/**/*", "cfn/**/*", "coverage"]
  },
  "jsRules": {
    "no-empty": false,
    "no-shadowed-variable": false,
    "object-literal-sort-keys": false
  }
}
